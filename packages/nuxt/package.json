{
  "name": "@wab-use/nuxt",
  "version": "1.0.39",
  "description": "Generic elements for Nuxt and Vue 3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Web-Artisan-Bros/wab-use.git",
    "directory": "packages/nuxt"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "types": "tsc src/runtime/stores/* -declaration --emitDeclarationOnly",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.12.2",
    "@nuxt/test-utils": "^3.13.1",
    "@wab-use/libs": "^1.0.8",
    "framer-motion": "^11.2.12",
    "motion": "^10.18.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.27"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.2.0",
    "@nuxt/eslint-config": "^0.3.13",
    "@nuxt/module-builder": "^0.6.0",
    "@nuxt/schema": "^3.11.2",
    "@types/node": "^20.12.11",
    "changelogen": "^0.5.5",
    "nuxt": "^3.11.2",
    "typescript": "latest",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.16"
  },
  "author": {
    "name": "Web Artisan Bros",
    "email": "info@webartisanbros.com"
  }
}
